package com.nanaimostudio.utils {	import com.nanaimostudio.utils.CoordUtil;	import flash.display.DisplayObject;	import flash.display.DisplayObjectContainer;	import flash.display.MovieClip;	import flash.geom.Point;		/**	 * ...	 * @author Boon Chew	 */	public class DisplayObjectUtil 	{		public static function reparent(target:DisplayObject, from:DisplayObjectContainer, to:DisplayObjectContainer, origin:Point = null):void		{			if (origin == null)			{				origin = new Point(target.x, target.y);			}						var newPos:Point = CoordUtil.localToLocal(from, to, origin);			target.x = newPos.x;			target.y = newPos.y;			to.addChild(target);		}	}}